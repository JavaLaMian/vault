<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vault.demo.dao.CreditDao">
    <insert id="bindCredit" parameterType="credit" useGeneratedKeys="true">
        <selectKey keyColumn="creId" resultType="integer" keyProperty="creId" order="AFTER">
            select LAST_INSERT_ID()
        </selectKey>
        <![CDATA[
          insert into credit values(#{creId},#{uId},#{depart},#{identity},#{hId},#{funds},#{photo})
        ]]>
    </insert>
</mapper>